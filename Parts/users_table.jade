div._card
    div.Header
        div.Title_Area جدول الأعضاء
        div.Navigation
            div.button.Reduire
                i(class="fa fa-minus" aria-hidden="true")
            div.button.Exit
                i(class="fa fa-times" aria-hidden="true")
    div.Body

        table.table.table-bordered.table-hover
            thead
                tr
                each val in ['#', 'إسم المستخدم', 'البريد', 'الإسم الشخصي', 'التحكم']
                    th= val
            tbody
                -for(var i = 0; i < 5 ; i++)
                    tr
                        each val2 in [i+1, 'المستخدم ' + i, 'بريد المست' + i, 'الإسم الشخصي' + i, 'التحكم' + i]
                            th= val2
